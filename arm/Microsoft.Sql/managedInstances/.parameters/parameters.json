        "enableRecuringVulnerabilityAssessmentsScans": {
            "value": false
        },
        "sendScanReportEmailsToSubscriptionAdmins": {
            "value": true
        },
        "sendScanReportToEmailAddresses": {
            "value": [
                "test1@contoso.com",
                "test2@contoso.com"
            ]
        },
        "roleAssignments": {
            "value": [
                {
                    "roleDefinitionIdOrName": "Reader",
                    "principalIds": [
                        "<<principalId>>"
                    ]
                }
            ]
        }
    }
