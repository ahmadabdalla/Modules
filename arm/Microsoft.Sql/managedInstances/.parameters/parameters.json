            "value": 32
        },
        "vCores": {
            "value": 4
        },
        "licenseType": {
            "value": "LicenseIncluded"
        },
        "hardwareFamily": {
            "value": "Gen5"
        },
        "dnsZonePartner": {
            "value": ""
        },
